#include "main.h"
#include <stdio.h>
/**
 * add - Adds two integers
 * @a: The first integer to add
 * @b: The second integer to add
 *
 * Return: The sum of the two integers
 */
int add(int a, int b)
{
return (a + b);
}
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
int result;
result = add(3, 4);
printf("The sum of 3 and 4 is %d.\n", result);
return (0);
}
