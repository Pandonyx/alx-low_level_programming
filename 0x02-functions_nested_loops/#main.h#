#ifndef MAIN_H
#define MAIN_H
int _putchar(char c);
#include "main.h"

/**
 * _islower - Checks for lowercase character
 * @c: The character to be checked
 *
 * Return: 1 if c is lowercase, 0 otherwise
 */
int _islower(int c)
{
    /* Check if the character is in the lowercase range (a-z) */
    if (c >= 'a' && c <= 'z')
    {
        return (1);
    }
    else
    {
        return (0);
    }
}
void print_alphabet(void);
void print_alphabet_x10(void);
#endif /* MAIN_H */
